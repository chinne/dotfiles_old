set -o vi

# aliases
unalias -a
alias ll="ls -al"
alias vi=vim
alias c=clear
alias p=python
alias python=python3
alias dotfiles_common="cd $HOME/repos/github.com/chinne/dotfiles"

source $HOME/repos/github.com/chinne/dotfiles/private/ssh
export PATH=$HOME/repos/github.com/chinne/dotfiles/scripts:$PATH
export PATH=/usr/local/go/bin:$PATH
